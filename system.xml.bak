<config
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd"
>
  <system>
    <section id="payment">
      <resource>Monei_MoneiPayment::config</resource>
      <group
        id="monei"
        translate="label"
        sortOrder="100"
        type="text"
        showInDefault="1"
        showInWebsite="1"
        showInStore="1"
      >
        <label>MONEI</label>
        <field
          id="version"
          translate="label"
          type="label"
          sortOrder="10"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Current Version</label>
          <backend_model>Monei\MoneiPayment\Model\Config\Source\ModuleVersion</backend_model>
        </field>
        <field
          id="heading1"
          translate="label"
          sortOrder="20"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Webservice Settings:</label>
          <frontend_model>Magento\Config\Block\System\Config\Form\Field\Heading</frontend_model>
        </field>
        <field
          id="mode"
          translate="label comment"
          sortOrder="30"
          type="select"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Mode</label>
          <comment
            >To define if we are going to connect with production or testing WS environment</comment
          >
          <source_model>Monei\MoneiPayment\Model\Config\Source\Mode</source_model>
        </field>
        <field
          id="test_account_id"
          translate="label comment"
          sortOrder="35"
          type="text"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Test Account Id</label>
          <validate>required-entry</validate>
          <backend_model>Monei\MoneiPayment\Model\Config\Backend\AccountId</backend_model>
          <depends>
            <field id="mode">1</field>
          </depends>
        </field>
        <field
          id="production_account_id"
          translate="label comment"
          sortOrder="35"
          type="text"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Production Account Id</label>
          <validate>required-entry</validate>
          <backend_model>Monei\MoneiPayment\Model\Config\Backend\AccountId</backend_model>
          <depends>
            <field id="mode">2</field>
          </depends>
        </field>
        <field
          id="test_api_key"
          translate="label comment"
          type="text"
          sortOrder="40"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Test Api Key</label>
          <validate>required-entry</validate>
          <depends>
            <field id="mode">1</field>
          </depends>
        </field>
        <field
          id="production_api_key"
          translate="label comment"
          type="text"
          sortOrder="40"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Production Api Key</label>
          <validate>required-entry</validate>
          <depends>
            <field id="mode">2</field>
          </depends>
        </field>
        <field
          id="language"
          translate="label comment"
          type="select"
          sortOrder="50"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Language</label>
          <source_model>Monei\MoneiPayment\Model\Config\Source\Language</source_model>
        </field>
        <field
          id="heading2"
          translate="label"
          sortOrder="60"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Basic settings:</label>
          <frontend_model>Magento\Config\Block\System\Config\Form\Field\Heading</frontend_model>
        </field>
        <field
          id="type_of_payment"
          translate="label comment"
          sortOrder="70"
          type="select"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Payment Action</label>
          <comment
            >Choose whether to only authorize payments or to authorize and capture them immediately.
            Note: Pre-authorization (Authorize option) is only supported for card payments, not for
            MBWay or Multibanco.</comment
          >
          <source_model>Monei\MoneiPayment\Model\Config\Source\TypeOfPayment</source_model>
        </field>
        <field
          id="confirmed_status"
          translate="label comment"
          type="select"
          sortOrder="80"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Captured Status</label>
          <source_model>Monei\MoneiPayment\Model\Config\Source\ProcessingStatus</source_model>
          <comment>Selectable with all the processing order statuses</comment>
        </field>
        <field
          id="pre_authorized_status"
          translate="label comment"
          type="select"
          sortOrder="90"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Authorized Status</label>
          <source_model>Monei\MoneiPayment\Model\Config\Source\PendingStatus</source_model>
          <comment>Selectable with all the pending order statuses</comment>
        </field>
        <field
          id="heading_emails"
          translate="label"
          sortOrder="95"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Email Settings:</label>
          <frontend_model>Magento\Config\Block\System\Config\Form\Field\Heading</frontend_model>
        </field>
        <field
          id="send_order_email"
          translate="label comment"
          sortOrder="96"
          type="select"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Send Order Email</label>
          <comment
            >Controls whether to send order confirmation emails after payment is confirmed</comment
          >
          <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
          <config_path>payment/monei/send_order_email</config_path>
        </field>
        <field
          id="send_invoice_email"
          translate="label comment"
          sortOrder="97"
          type="select"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Send Invoice Email</label>
          <comment
            >Controls whether to send invoice emails for automatically created invoices. Does not
            affect manually created invoices, for which merchants can choose to notify customers
            during creation.</comment
          >
          <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
          <config_path>payment/monei/send_invoice_email</config_path>
        </field>
        <field
          id="heading3"
          translate="label"
          sortOrder="100"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Monei Redirect:</label>
          <frontend_model>Magento\Config\Block\System\Config\Form\Field\Heading</frontend_model>
        </field>
        <field
          id="active"
          translate="label comment"
          sortOrder="110"
          type="select"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Enable</label>
          <comment>Select 'Yes' if you want to enable this payment method. Otherwise 'No'.</comment>
          <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
        </field>
        <field
          id="title"
          translate="label comment"
          type="text"
          sortOrder="140"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Title</label>
          <comment>Text that will be displayed in checkout</comment>
          <depends>
            <field id="active">1</field>
          </depends>
        </field>
        <field
          id="description"
          translate="label comment"
          type="text"
          sortOrder="150"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Description</label>
          <comment>Small description that will be displayed in checkout (Under title)</comment>
          <depends>
            <field id="active">1</field>
          </depends>
        </field>
        <field
          id="allowspecific"
          translate="label comment"
          sortOrder="160"
          type="allowspecific"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Allowed Countries</label>
          <source_model>Magento\Payment\Model\Config\Source\Allspecificcountries</source_model>
          <depends>
            <field id="active">1</field>
          </depends>
        </field>
        <field
          id="specificcountry"
          translate="label comment"
          sortOrder="170"
          type="multiselect"
          showInDefault="1"
          showInWebsite="1"
          showInStore="1"
        >
          <label>Payment from Specific Countries</label>
          <source_model>Magento\Directory\Model\Config\Source\Country</source_model>
          <can_be_empty>1</can_be_empty>
          <depends>
            <field id="active">1</field>
            <field id="allowspecific">1</field>
          </depends>
        </field>
        <field
          id="sort_order"
          translate="label comment"
          sortOrder="180"
          type="text"
          showInDefault="1"
          showInWebsite="1"
        >
          <label>Sort Order</label>
          <tooltip>In which order the payment method will be displayed in checkout</tooltip>
          <frontend_class>validate-number</frontend_class>
          <depends>
            <field id="active">1</field>
          </depends>
        </field>
